using Gtk 4.0;
using Adw 1;

template $DashboardPage : Gtk.Box {
  hexpand: true;
  vexpand: true;
  orientation: vertical;
  Adw.HeaderBar {
    styles ['flat']
    title-widget: Label {
      styles ['title']
      label: 'Dashboard';
    };
  }
  Adw.Clamp {
    hexpand: true;
    vexpand: true;
    orientation: vertical;
    Gtk.Box {
      orientation: vertical;
      halign: center;
      valign: center;
      Gtk.Label {
        label: 'Dashboard';
      }
    }
  }
}

